{
  "name": "woonuxt",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host localhost",
    "dev:ssl": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --host localhost --https --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "serve": "nuxi generate && npx serve .output/public -l 8080"
  },
  "dependencies": {
    "@heroicons/vue": "^2.1.5",
    "@nuxtjs/robots": "^5.5.6",
    "@nuxtjs/sitemap": "^7.4.7",
    "@stripe/stripe-js": "^8.3.0",
    "@vueform/slider": "^2.1.10",
    "@vueuse/core": "^14.0.0",
    "swiper": "^12.0.3",
    "vue-carousel": "^0.18.0",
    "vue-matomo": "^4.2.0"
  },
  "devDependencies": {
    "@nuxt/icon": "^2.1.0",
    "@nuxt/image": "^2.0.0",
    "@nuxtjs/i18n": "^10.2.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@tailwindcss/typography": "^0.5.19",
    "nuxt": "^4.2.0",
    "nuxt-graphql-client": "^0.2.46",
    "nuxt-gtag": "^4.1.0",
    "prettier": "^3.6.2",
    "woonuxt-settings": "^1.2.1"
  },
  "overrides": {
    "sharp": "0.33.5"
  }
}
