    "@nuxtjs/robots": "^5.0.1",
    "@nuxtjs/sitemap": "^7.0.0",
    "@stripe/stripe-js": "^4.3.0",
    "@vueform/slider": "^2.1.10",
    "@vueuse/core": "^12.2.0",
    "swiper": "^11.1.14",
    "vue-carousel": "^0.18.0",
    "vue-matomo": "^4.2.0"
  },
  "devDependencies": {
    "@nuxt/icon": "^1.5.0",
    "@nuxt/image": "1.7.1",
    "@nuxtjs/i18n": "^8.5.1",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@tailwindcss/typography": "^0.5.14",
    "nuxt": "^3.13.0",
    "nuxt-graphql-client": "^0.2.36",
    "nuxt-gtag": "^3.0.2",
    "prettier": "^3.3.3",
    "woonuxt-settings": "^1.0.60"
