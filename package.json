{
  "name": "woonuxt",
  "private": true,
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host localhost",
    "dev:ssl": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --host localhost --https --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "serve": "nuxi generate && npx serve .output/public -l 8080",
    "clean": "nuxi cleanup",
    "clean:dev": "nuxi cleanup && npm run dev",
    "update-categories": "tsx woonuxt_base/scripts/update-categories.ts",
    "update-products": "tsx woonuxt_base/scripts/update-products.ts",
    "update-brands": "tsx woonuxt_base/scripts/update-brands.ts",
    "update-all": "tsx woonuxt_base/scripts/update-all.ts"
  },
  "dependencies": {
    "@heroicons/vue": "^2.1.5",
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/ion": "^1.2.6",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@nuxtjs/robots": "^5.5.6",
    "@nuxtjs/sitemap": "^7.4.7",
    "@pinia/nuxt": "^0.11.3",
    "@stripe/stripe-js": "^8.3.0",
    "@tato30/vue-pdf": "^1.11.5",
    "@vueform/slider": "^2.1.10",
    "@vueuse/core": "^14.0.0",
    "pinia": "^3.0.4",
    "swiper": "^12.0.3",
    "vue-carousel": "^0.18.0",
    "vue-matomo": "^4.2.0"
  },
  "devDependencies": {
    "@nuxt/icon": "^2.1.0",
    "@nuxt/image": "^2.0.0",
    "@nuxtjs/i18n": "^10.2.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@tailwindcss/typography": "^0.5.19",
    "dotenv": "^17.2.3",
    "nuxt": "^4.2.0",
    "nuxt-gtag": "^4.1.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "woonuxt-settings": "^1.2.1"
  },
  "overrides": {
    "sharp": "0.33.5"
  }
}
